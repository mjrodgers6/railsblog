<%= form_for @profile do |f| %>
  <%= f.text_field :name, {placeholder: 'super hero name'} %>
  <%= f.text_field :identity, {placeholder: 'real name'} %>
  <%= f.text_field :power, {placeholder: 'what are your superpowers'} %>
  <%= f.text_field :contact, {placeholder: 'how do we contact your'} %>
  <%= f.file_field :avatar %>
  <%= f.submit :update %>
  <% end %>